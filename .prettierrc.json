{
  "singleQuote": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^[react*]",
    "^[react*/**]",
    "<BUILT_IN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@/*",
    "^[a-z|A-Z]",
    "",
    "^~/*",
    "^./(.*)$",
    "^../(.*)$",
    "./*.module.css"
  ]
}